{
  "name": "@coffic/cosy-framework",
  "version": "1.0.0",
  "description": "Laravel-inspired framework for Electron applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "electron",
    "laravel",
    "framework",
    "dependency-injection",
    "ioc",
    "middleware",
    "router"
  ],
  "author": "Coffic Team",
  "license": "MIT",
  "dependencies": {
    "electron": "^27.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./application": {
      "import": "./dist/application/index.js",
      "require": "./dist/application/index.js",
      "types": "./dist/application/index.d.ts"
    },
    "./container": {
      "import": "./dist/container/index.js",
      "require": "./dist/container/index.js",
      "types": "./dist/container/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./router": {
      "import": "./dist/router/index.js",
      "require": "./dist/router/index.js",
      "types": "./dist/router/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./facades": {
      "import": "./dist/facades/index.js",
      "require": "./dist/facades/index.js",
      "types": "./dist/facades/index.d.ts"
    }
  }
}
